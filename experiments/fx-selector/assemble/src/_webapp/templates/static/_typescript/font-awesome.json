class WebFont {

	static getCode(icon : string) {
		var code : string = IconMoonFont.getCode(icon);
		if (code.length() == 0) {
			code = FontAwesomeFont.getCode(icon);
			if (code.length() == 0) {
				code = "";
			}
		}
		return code;
	}
}
